<main>
 <div class="row">
  <div class="col-12">
    <section >
     <div class="row ">
     
         {{#each navigation.links}}
               <div class="col-2">
                <div class="nav">
               <div class="nav flex-column mb-3">
                  <div class="nav-item">
                <a href=" {{href}}" class="nav-link {{#if href}}active{{else}}disabled{{/if}} " >{{text}}</a>
               </div>
                       </div>
                 </div>
                  </div>  
               {{/each}}
     </div>
    </section>
   
       <div class="row g-5" >
    <section>
   
        <div class="col-8" >
       <div class="ms-2"><h2>{{header}}</h2>
       </div>
        {{#with contacts_data}}
        <h3 class="ms-2">{{title}}</h3>
        <div class="ms-2">
         {{#each buttons}}
         
         {{#if isPrimary}}
         <a class="btn btn-warning text-light  btn-sm">{{text}}</a>{{/if}}
         {{#if isOutline}}
         <a class="btn btn-outline-warning btn-sm ">{{text}}</a>{{/if}}
      
                {{/each}}
          </div>
                {{#with form}}
                <div class="row">
                  {{#with input}}
                  <div class="col-4 mt-3 ms-2">
                    <label class="form=label">{{surname.label}}</label>
                    <input class="form-control-sm" type="text" value="{{surname.placeholder}}">
                    <div class="form-text" >{{ surname.caption}}</div>
                  </div>
                   <div class="col-4 mt-3 ms-5 ps-3">
                    <label class="form=label">{{name.label}}</label>
                    <input class="form-control-sm" type="text" value="{{name.placeholder}}" >
                    <div class="form-text" >{{ name.caption}}</div>
                  </div>
                   <div class="col-4 mt-2 ms-2 mb-4">
                    <label class="form=label">{{ phone.label}}</label>
                    <input class="form-control-sm" value="{{phone.placeholder}}">
                    <div class="form-text" >{{  phone.caption}}</div>
                  </div>
                   <div class="col-4 mt-4 pt-2 ms-5 ps-3 mb-4">
                    <label class="form=label">{{  email.label}}</label>
                    <input class="form-control-sm" value="{{  email.placeholder}}">
                    <div class="form-text" >{{  email.caption}}</div>
                  </div>
                  <hr>
                  
                  {{#each switch}}
                  <div class="col-8 ">
                 <div class="form-check form-switch">
                <label class="form-check-label ">{{text}}</label>
                 <input class="from" type="checkbox">
                </div>
                  </div>
                  {{/each}}
                   </div>
                  {{/with}}
                </div>
                {{#each switch}}
                <div class="col-8 ms-2">
                  <div class="form-check form-switch">
  <input class="form-check-input" type="checkbox">
   <label class="form-check-label">{{text}}</label>

</div>
                </div>
                {{/each}}

                {{/with}}
               {{/with}}
       </div>
       </div>
      </div>
        </section>
        <hr>
</div>


<section>
   <div class="row">
  <div class="col-8">
  {{#with delivery}}
  
  <h3 class="ms-2">{{title}}</h3>
  
    
    <div class="card ms-2">
       
      <div class="card-body">
         <div class="row">
          {{#each select}}
    <div class="col-6">
        <label class="form-label ms-2">{{label}}</label>
 <select class="form-select form-select-sm" >
   <option value="placeholder">{{placeholder}}</option>
   {{#each options}}
     <option value="{{value}}">{{text}}</option>
   </div>
     {{/each}}
  </select>
   </div>
      {{/each}}
   
   </div>
 </div>
 </div>
 
<div class="mt-3 ms-2">

 <div class="col-12">
   {{#each  delivery_type}}
  <div class="form-check">
    <input type="radio" class="form-check-input">

<label class="form-check-label">{{type}}</label>
 {{/each}}
 </div>
 </div>
</div>
<hr>

<div class="mb-3">
  <div class="col-8">
    {{#with payment}}
    <h3>{{title}}</h3>
    {{#each list}}
    <div class="form-check">
      <input type="radio" class="form-check-input">
      <label class="form-check-label">{{type}}</label>
      <div class="form-text">{{ description}}</div>

    </div>
    {{/each}}
    {{/with}}
  </div>
</div>
 <hr>


<div class="row">
  <div class="col-12">
  {{#with  contact}}
  <h3>{{title}}</h3>
<div class="row">
       <div class="col-4 ms-2 me-2">
                    <label class="form=label">{{surname.label}}</label>
                    <input class="form-control-sm" type="text" value="{{surname.placeholder}}">
                    <div class="form-text" >{{ surname.caption}}</div>
                  </div>
                   <div class="col-4  ms-5 ps-4">
                    <label class="form=label">{{name.label}}</label>
                    <input class="form-control-sm" type="text" value="{{name.placeholder}}" >
                    <div class="form-text" >{{ name.caption}}</div>
                  </div>
                   <div class="col-4  ms-2 mt-4 me-2">
                    <label class="form=label">{{ phone.label}}</label>
                    <input class="form-control-sm" value="{{phone.placeholder}}">
                    <div class="form-text" >{{  phone.caption}}</div>
                  </div>
                   <div class="col-4  ms-5 ps-4 mt-5">
                    <label class="form=label">{{  email.label}}</label>
                    <input class="form-control-sm" value="{{  email.placeholder}}">
                    <div class="form-text" >{{  email.caption}}</div>
                  </div>
 </div>
                  {{/with}}
                  <hr>
               
                  {{#with coment}}
                   <div class="col-12">
                  <label class="form=label">{{label}}</label>
                  
                   <input class="form-control"  value="{{placeholder}}">
                 
                  </div>
                  {{/with}}
                 
 {{/with}}
   </div>
</div>
 <hr>

 <section>
  <div class="row">
  <div class="col-12">
 {{#with coupon}}
<h3>{{title}}</h3>

{{#with select}}
<div class="col-12">
{{#with availabel_coupon}}
<div class="col-12">
<label class="form-label">{{label}}</label>
 <select class="form-select form-select-lg" >
   <option value="placeholder">{{placeholder}}</option> 
   {{#each options}}
   
   <option value="{{value}}">{{text}}</option>
   </div>
   </div>
  
   {{/each}}
  </select>
 </div>
{{/with}}
{{#with  friend_coupon}}
<label class="form-label">{{label}}</label>
 <select class="form-select form-select-sm" >
   <option value="placeholder">{{placeholder}}</option>
   {{#each options}}
      <option value="{{value}}">{{text}}</option>
     {{/each}}
  </select>
{{/with}}
</div>
{{/with}}

{{#with add}}

{{#with friend}}
    <label class="form-label">{{label}}</label>
   <input class="form-control form-control-lg" value="{{placeholder}}">
   <div class="form-text">{{caption}}</div>

{{/with}}
{{#with colleague}}
<label class="form-label">{{label}}</label>
   <input class="form-control form-control-sm" value="{{placeholder}}">
   <div class="form-text">{{caption}}</div>
{{/with}}
{{/with}}
 {{/with}}
 </div>
</div>
 </section>
<hr>

<section>
  <div class="row">
    <diw class="col-12">
  {{#with quiz}}
<h6>{{title}}</h6>

{{#with  preference}}
  <small><div class="form-label">{{title}}</div></small>

  <div class="form-check">
  
     <label class="form-check-label">
       {{#each options}}
    <input type="radio" class="form-check-input">
   <ul class="list-group list-group-flush">
      <li class="list-group-item">{{this}}</li>
    </ul>{{/each}} </label>
       
 </div>
{{/with}}

{{#with your_expirience}}
<small><label class="form-label">{{label}}</label></small>
<input class="form-control">
{{/with}}

{{#with purchase_frequency}}
<small><label class="form-label">{{label}}</label></small>
 <select class="form-select form-select-sm" >
   <option value="placeholder">{{placeholder}}</option> 
   {{#each options}}
   <option value="{{value}}">{{text}}</option> 
   {{/each}}
{{/with}}


 <div class="row gt-5">
<div class="col-12">
{{#with factors}}
<div class="form-label">{{title}}</div>
<label class="form-check-label"></label>
{{#each options}}
 <input type="checkbox" class="form-check-input">
   <ul class="list-group list-group-flush">
      <li class="list-group-item">{{this}}</li>
    </ul> </label>
{{/each}}
{{/with}}
</div>
</div>
{{/with}}
    </diw>
  </div>
</section>
<hr>
<section>
  <div class="row">
   
    {{#each footer}}
    <div class="col-2 ms-3 list-group">
      <ul class="nav flex-column">
        {{#each this}}
        <li class="nav-item my-2">
          <a href="{{link}} class="nav-link"{{#if @last}}disabled{{/if}} list-item" target="_blank">{{text}}</a>
        </li>
        {{/each}}
      </ul>
    </div>
    {{/each}}
  </div>
  </div>
</section>


 </section>
</main>

  